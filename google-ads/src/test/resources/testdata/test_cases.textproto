# Modify element in a repeated field
test_cases {
  description: "Modify element in a repeated field"
  original_resource {
    foos {
      num: 1
    }
    foos {
      num: 2
    }
  }
  modified_resource {
    foos {
      num: 1
    }
    foos {
      num: 3
    }
  }
  expected_mask {
    paths: "foos"
  }
}

# Modify element in a nested repeated field
test_cases {
  description: "Modify element in a nested repeated field"
  original_resource {
    foos {
      bars {
        num: 1
      }
    }
  }
  modified_resource {
    foos {
      bars {
        num: 2
      }
    }
  }
  expected_mask {
    paths: "foos"
  }
}

