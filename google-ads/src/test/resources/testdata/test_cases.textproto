
# Serialized TestSuite proto

# Modify scalar in a wrapper proto
test_cases {
  description: "Modify scalar in a wrapper proto"
  original_resource {
    wrapper {
      value: "a"
    }
  }
  modified_resource {
    wrapper {
      value: "b"
    }
  }
  expected_mask {
    paths: "wrapper"
  }
}

